{
  "profile": {
    "id": "{{WORKFLOW_ID}}",
    "name": "{{WORKFLOW_NAME}}",
	  "project": "{{WORKFLOW_PROJECT}}",
	  "organization": "{{WORKFLOW_ORG}}",
	  "logos": [
		  "{{LOGO1}}",
		  "{{LOGO2}}"
	  ],
	  "mode": "entity",
	  "options": {
	    "line_separator": "{{LINE_SEPARATOR}}"
    }
  },
  "metadata": {
    "entities": [
      {
        "handler": "{{METADATA_SOURCE_HANDLER}}",
        "source": "{{METADATA_SOURCE}}"
      }
    ],
    "contacts" : [
      {
        "handler": "{{METADATA_CONTACTS_HANDLER}}",
        "source": "{{METADATA_CONTACTS}}"
      }
    ]
  },
  "software": [
	{
		"id": "220801-geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_URL}}",
			"user": "{{GEONETWORK_USER}}",
			"pwd": "{{GEONETWORK_PASSWORD}}",
			"version": "{{GN_VERSION}}",
			"logger": "{{LOG}}"
		}
	},
		{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
			"email": "{{GMAIL_USER}}"
		},
		"properties" : {}
	},    
	{
		"id": "220801-geoserver",
		"type": "output",
		"software_type": "geoserver",
		"parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "{{LOG}}"
		},
		"properties" : {
			"workspace": "{{GEOSERVER_WORKSPACE}}"
		}
	}
  ],
  "actions": [
	{
      "id": "geometa-create-iso-19115",
	  "options": {
		"logo": true
	  },
      "run": true
    },
	{
	  "id": "geonapi-publish-iso-19139",
      "run" : true,
      	     "options" : {
	   "group":"{{GROUP}}"
	   }
	},
	{
	  "id": "geosapi-publish-ogc-services",
	  "run": true,
	  "options" : {
	    "createWorkspace": true,
	    "createStore": true
	  }
	}
  ]
}
