{
    "id": "G2OI_COI_PGRNC_workflow",
    "profile": {
      "project": "G2OI project",
      "organization": "COI & IRD",
          "logos": [
          "https://drive.google.com/file/d/1HstEki8xQ8SeCZpOXX9LL8YVS1zTA_Dm" 
      ]
    },
    "options": {
      "line_separator": "_\n"
    },
    "mode": "entity",
    "dependencies": {
      "packages": {
        "cran": [
          "gsheet"
        ],
        "cran_force_install" : false,
        "github": [],
        "github_force_install": false
      },
      "scripts": []
    },
    "metadata": {
      "entities": {
        "handler": "gsheet",
        "source": "https://docs.google.com/spreadsheets/d/1gK2N27VBptzLzYY86zJVoeViO_rsTqvz4w4U7elLkZ4/edit#gid=1311954662"
        },
      "contacts" : {
        "handler": "gsheet",
        "source": "https://docs.google.com/spreadsheets/d/1DtGR8cHAkro12DOFZn40cehfU9LoI9l7wVUCHfHH-xY/edit?usp=sharing"
        }
    },
    "software": [
              {
          "id": "g2oi-coi-csw",
          "type": "output",
          "software_type": "csw",
          "parameters": {
              "url": "{{GEONETWORK_CSW_URL}}",
              "user": "{{GEONETWORK_USER}}",
              "pwd": "{{GEONETWORK_PASSWORD}}",
              "serviceVersion": "2.0.2",
              "logger": "DEBUG"
          }
      },
      {
          "id": "g2oi-coi-geonetwork",
          "type": "output",
          "software_type": "geonetwork",
          "parameters": {
              "url": "{{GEONETWORK_URL}}",
              "user": "{{GEONETWORK_USER}}",
              "pwd": "{{GEONETWORK_PASSWORD}}",
              "version": "4.2.0",
              "logger": "DEBUG"
          }
      },
      {
          "id": "g2oi-coi-geoserver",
          "type": "output",
          "software_type": "geoserver",
          "parameters": {
              "url": "{{GEOSERVER_URL}}",
              "user": "{{GEOSERVER_USER}}",
              "pwd": "{{GEOSERVER_PASSWORD}}",
              "logger": "DEBUG"
          },
        "properties" : {
              "workspace": "COI"
        }
      },
          {
          "id": "googledrive",
          "type": "input",
          "software_type": "googledrive",
          "parameters": {
              "email": "{{GMAIL_USER}}",
              "token": ""
          },
          "properties" : {}
      }
    ],
    "actions": [
      {
      "id": "geometa-create-iso-19115",
      "run": true,
        "options": {
            "logo": true,
            "doi": true,
            "doi_thumbnail": true,
            "addfeatures": false
        }
      },
      {
        "id": "geometa-create-iso-19110",
        "run": false
      },
      {
        "id": "geonapi-publish-iso-19139",
        "run" : false,
                 "options" : {
         "group":"{{GROUP}}"
         }	},
             {
        "id": "ows4R-publish-iso-19139",
        "run": true
      },
      {
        "id": "geosapi-publish-ogc-services",
        "run": true,
        "options" : {
          "createWorkspace": true,
          "createStore": true
        }
      }
   ]
  }