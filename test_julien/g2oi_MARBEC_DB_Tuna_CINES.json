{
  "profile": {
    "id": "metadata_seatizen",
    "name": "Metadata and dat access for Seatizen project",
	  "project": "Seatizen",
	  "organization": "Ifremer & IRD",
	  "logos": [
        "https://drive.google.com/uc?id=177eRceCLr_VmfAf9SWK3a4MBZix_zHjH"
	  ],
	  "mode": "entity",
	  "options": {
	    "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities": {
      "handler": "dbi",
      "source": "select * from metadata.metadata_dcmi where id_metadata = 1"
    },
    "contacts" : {
      "handler": "gsheet",
      "source": "https://docs.google.com/spreadsheets/d/144NmGsikdIRE578IN0McK9uZEUHZdBuZcGy1pJS6nAg/edit?usp=sharing"
    }
  },
  "software": [
    {
        "id": "Postgis",
        "type": "input",
        "software_type": "dbi",
        "parameters": {
            "drv": "{{DB_DRV}}",
            "user": "{{DB_TUNA_USER}}",
            "password": "{{DB_TUNA_PWD}}",
            "host": "{{DB_TUNA_HOST}}",
            "port": "{{DB_PORT}}",
            "dbname": "{{DB_TUNA_NAME}}"
        }
  },
    {
		"id": "CINES-geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_OREME_URL}}",
			"user": "{{GEONETWORK_OREME_USER}}",
			"pwd": "{{GEONETWORK_OREME_PASSWORD}}",
			"version": "3.0.4.0",
			"logger": "DEBUG"
		}
    },
    {
        "id": "CINES-geoserver",
        "type": "output",
        "software_type": "geoserver",
        "parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "DEBUG"
        },
		"properties" : {
			"workspace": "SEATIZEN",
			"store": "Reef_database"
		}
    },
		{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
			"email": "{{GMAIL_FIRMS_USER}}",
			"token": ""
		},
		"properties" : {}
	}
  ],
  "actions": [
	{
    "id": "geometa-create-iso-19115",
    "run": true,
	  "options": {
  		"logo": true,
  		"doi": true,
  		"doi_thumbnail": true,
  		"addfeatures": false
	  }
    },
	{
	  "id": "geometa-create-iso-19110",
	  "run": false
	},
	{
	  "id": "geonapi-publish-iso-19139",
      "run" : true,
      	     "options" : {
	   }	},
	{
	  "id": "geosapi-publish-ogc-services",
	  "run": false,
	  "options" : {
	    "createWorkspace": true,
	    "createStore": true
	  }
	}
 ]
}