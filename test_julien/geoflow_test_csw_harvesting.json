			"url": "{{GEONETWORK_URL}}",
			"user": "{{GEONETWORK_USER}}",
			"pwd": "{{GEONETWORK_PASSWORD}}",
      "version": "4.2.0.0",
			"logger": "DEBUG"
      }
    }
