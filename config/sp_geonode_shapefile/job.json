{
  "profile": {
    "id": "Geonode",
    "name": "Geonode test",
	  "project": "Test geoflow project to publish shapefile in Geonode",
	  "organization": "IRD",
	  "environment": {
        "file": ".env",
        "hide_env_vars": ["MOTDEPASSE"]
        },  
	  "logos": [
		  "https://www.gebco.net/data_and_products/gebco_web_services/web_map_service/mapserv?request=getmap&service=wms&BBOX=-90,-180,90,360&crs=EPSG:4326&format=image/jpeg&layers=gebco_latest_sub_ice_topo&width=1200&height=600&version=1.3.0",
		  "http://svs.gsfc.nasa.gov/cgi-bin/wms?SERVICE=WMS&VERSION=1.3.0&REQUEST=GetMap&LAYERS=3191_21982&FORMAT=image/png&WIDTH=1024&HEIGHT=512&CRS=CRS:84&BBOX=-180.0,-90.0,180.0,90.0&STYLES="
	  ],
	  "mode": "entity",
	  "options": {
	    "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities": [
      {
        "handler": "gsheet",
        "source": "https://docs.google.com/spreadsheets/d/1YcEK97fRGJFf7P14d9yAfgUOzOzrVxN_q27KyAx40ck/edit?usp=sharing"
      }
    ],
    "contacts" : [
      {
        "handler": "gsheet",
        "source": "https://docs.google.com/spreadsheets/d/1BqlXwA2fKiRuozNAQhBb_PbQVSPTCfl8_Q9rfM8E2ws/edit?usp=sharing"
      }
    ]
  },
  "software": [
    	{
		"id": "geonode",
		"type": "output",
		"software_type": "geonode",
		"parameters": {
			"url": "{{GEONODE_URL}}",
			"user": "{{GEONETWORK_USER}}",
			"pwd": "{{GEONETWORK_PASSWORD}}",
			"logger": "DEBUG"
		}
	}
  ],
  "actions": [
  	{
  	  "id": "geonode4R-publish-ogc-services",
  	  "run": true
  	}
  ]
}
