      }
    ]
  },
  "software": [
    {
  		"id": "sandbox-zenodo-amcc",
		  "type": "output",
		  "software_type": "zenodo",
		  "parameters": {
    		"url": "https://sandbox.zenodo.org/api",
    		"token": "{{ ZENODO_SANDBOX_TOKEN }}",
    		"logger": "DEBUG"
		  },
		  "properties": {
		    "clean": {
		      "run": false
		    }
		  }
		},
    {
      "id": "googledrive",
      "type": "input",
      "software_type": "googledrive",
      "parameters": {
          "email": "{{GMAIL_USER}}",
          "token": ""
      },
      "properties" : {}
  }
  ],
  "actions": [
    {
      "id": "zen4R-deposit-record",
      "run": true,
      "options": {
        "update_files": true,
        "depositWithFiles": true,
        "communities": "",
        "deleteOldFiles": true,
        "publish":false
      }
    }
  ]
