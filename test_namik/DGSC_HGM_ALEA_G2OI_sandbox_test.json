  	"logos": [
  		""
  	],
  	"mode": "entity"
  },
  "metadata": {
    "entities": [
      {
        "handler": "gsheet",
          "source": "https://docs.google.com/spreadsheets/d/1qt5ku-hDIoJ4rtIRlhg3m2J-wgkfUcvGZQJ1LZ0BB6A/edit#gid=0"
      }
    ],
    "contacts" : [
