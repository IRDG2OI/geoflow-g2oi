{
  "profile": {
    "id": "{{WORKFLOW_ID}}",
    "name": "{{WORKFLOW_NAME}}",
	  "project": "{{WORKFLOW_PROJECT}}",
	  "organization": "{{WORKFLOW_ORG}}",
	  "logos": [
		  "https://gred.ird.fr/var/ird/storage/images/media/ird-sites-d-unites-de-recherche/gred/images/logos/logo-ird/209819-2-fre-FR/logo-ird.png"
	  ],
	  "mode": "entity",
	  "options": {
	    "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities": [
      {
        "handler": "gsheet",
        "source": "{{METADATA_SOURCE}}"
      }
    ],
    "contacts" : [
      {
        "handler": "gsheet",
        "source": "{{METADATA_CONTACTS}}"
      }
    ]
  },
  "software": [
	{
		"id": "220801-geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_URL}}",
			"user": "{{GEONETWORK_USER}}",
			"pwd": "{{GEONETWORK_PASSWORD}}",
			"version": "4.2.0",
			"logger": "DEBUG"
		}
	},
		{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
			"email": "{{GMAIL_USER}}"
		},
		"properties" : {}
	},    
	{
		"id": "220801-geoserver",
		"type": "output",
		"software_type": "geoserver",
		"parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "DEBUG"
		},
		"properties" : {
			"workspace": "{{GEOSERVER_WORKSPACE}}"
		}
	}
  ],
  "actions": [
	{
      "id": "geometa-create-iso-19115",
	  "options": {
		"logo": true
	  },
      "run": true
    },
	{
	  "id": "geonapi-publish-iso-19139",
      "run" : true,
      	     "options" : {
	   "group":"{{GROUP}}"
	   }
	},
	{
	  "id": "geosapi-publish-ogc-services",
	  "run": true,
	  "options" : {
	    "createWorkspace": true,
	    "createStore": true
	  }
	}
  ]
}
