{
  "profile": {
    "id": "FMR",
    "mode": "entity",
  	"project": "FMR",
  	"organization": "IRD",
	  "environment": {
        "file": ".env",
        "hide_env_vars": ["MOTDEPASSE"]
        },  	
  	"logos": [
  		  		"https://umr-entropie.ird.nc/application/files/cache/thumbnails/b082b6a80abd8aea812aaf8090cf6c91.png",
  		  		"https://en.ird.fr/sites/ird_fr/files/2019-08/logo_IRD_2016_BLOC_UK_COUL.png"
  	],
  	"options": {
  	  "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities":
      {
      "handler": "excel",
      "source": "https://drive.ird.fr/s/BkHTnofyCi5tmWa/download/METADATA_FMR_202209_.xlsx"
      },
    "contacts" : 
      {
      "handler": "excel",
      "source": "https://drive.ird.fr/s/tfQxYSBtio2nK9n/download/geoflow_g2oi_contacts.xlsx"
      }
  },
  "software": [
	{
		"id": "geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_URL}}",
			"user": "{{GEONETWORK_USER}}",
			"pwd": "{{GEONETWORK_PASSWORD}}",
			"version": "4.2.1",
			"logger": "DEBUG"
		}
	},
	{
		"id": "geoserver",
		"type": "output",
		"software_type": "geoserver",
		"parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "DEBUG"
		},
		"properties" : {
			"workspace": "IRD_ENTROPIE",
			"store": "MAYOTTE_FMR"
		}
	}
  ],
 "actions": [
    {
      "id": "geometa-create-iso-19115",
      "options" : {
        "logo": true
      },
      "run": true
    },
        {
      "id": "geonapi-publish-iso-19139",
      "run" : true,
      	     "options" : {
	   "group":"{{GROUP}}"
	   }
	   },
        {
      "id": "geosapi-publish-ogc-services",
      "run" : false,
      	  	  "options" : {
		  "createWorkspace": true,
		  "createStore": true
		  }
    }
  ]
}
