{
  "profile": {
    "id": "FMR",
    "mode": "entity",
  	"project": "FMR",
  	"organization": "IRD",
  	"logos": [
  		  		"https://umr-entropie.ird.nc/application/files/cache/thumbnails/b082b6a80abd8aea812aaf8090cf6c91.png",
  		  		"https://en.ird.fr/sites/ird_fr/files/2019-08/logo_IRD_2016_BLOC_UK_COUL.png"
  	],
  	"options": {
  	  "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities":
      {
      "handler": "gsheet",
      "source": "https://docs.google.com/spreadsheets/d/1WOfzrpf4b7lS8yRQerWCYcTxPmZ38ev9Ed6uhhtvVhQ/edit?usp=sharing"
      },
    "contacts" : 
      {
      "handler": "gsheet",
      "source": "{{METADATA_CONTACTS}}"
      }
  },
  "software": [
	{
		"id": "geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"version": "4.2.1",
			"logger": "DEBUG"
		}
	},
	{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
			"email": "{{GMAIL_USER}}",
			"token": ""
		},
		"properties" : {}
	},
	{
		"id": "geoserver",
		"type": "output",
		"software_type": "geoserver",
		"parameters": {
			"url": "{{GEOSERVER_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "DEBUG"
		},
		"properties" : {
			"workspace": "IRD_ENTROPIE",
			"store": "MAYOTTE_FMR"
		}
	}
  ],
 "actions": [
    {
      "id": "geometa-create-iso-19115",
      "options" : {
        "logo": true
      },
      "run": true
    },
        {
      "id": "geonapi-publish-iso-19139",
      "run" : true,
      	     "options" : {
	   "group":"{{GROUP}}"
	   }
	   },
        {
      "id": "geosapi-publish-ogc-services",
      "run" : true,
      	  	  "options" : {
		  "createWorkspace": true,
		  "createStore": true
		  }
    }
  ]
}
