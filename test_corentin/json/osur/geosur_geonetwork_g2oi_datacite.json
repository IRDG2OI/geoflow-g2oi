{
  "profile": {
    "id": "geosur",
  	"project": "geosur",
  	"organization": "G2OI",
  	"logos": [
  	],
  	"mode": "entity",
  	"options": {
  	  "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities":
      {
        "handler": "gsheet",
        "source": "https://docs.google.com/spreadsheets/d/1JfbGypt0GTB7GsWNfdcwVB3hlSbqePDoDFGnf-xNzBA/edit?usp=sharing"
      },
      "contacts" : {
      "handler": "csv",
      "source": "test_corentin/metadata/osur/metadata_contacts_geoflow.csv"
    }
  },
  "software": [
    {
		"id": "geosur-geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_DATACITE_URL}}",
			"user": "{{GEONETWORK_DATACITE_USER}}",
			"pwd": "{{GEONETWORK_DATACITE_PASSWORD}}",
			"version": "4.2.1",
			"logger": "DEBUG"
		}
	},
    	{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
    "email": "{{GMAIL_USER}}",
			"token": ""
		},
		"properties" : {}
	}
  ],
 "actions": [
    {
      "id": "geometa-create-iso-19115",
      "options" : {
        "logo": true
      },
      "run": true
    },
        {
      "id": "geonapi-publish-iso-19139",
      "run" : true,
      "options": {
        "create_doi_on_datacite": true
      }
	   }
  ]
}