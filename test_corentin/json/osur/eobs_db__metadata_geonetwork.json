{
  "profile": {
    "id": "0930-eobs-metadata",
    "name": "Fiche metadonn√©e depuis vue eobs",
  	"project": "eobs",
  	"organization": "G2OI",
  	"logos": [
  		  		"https://drive.google.com/uc?id=16D4qCIKtRBD94-wOcMfM1HcA9c6uKM50",
