{
  "profile": {
    "id": "eobs-geosur-obs",
  	"project": "geosur",
  	"organization": "G2OI",
  	"logos": [
  	],
  	"mode": "entity",
  	"options": {
  	  "line_separator": "_\n"
    }
  },
  "metadata": {
    "entities":
      {
      "handler": "csv",
      "source": "https://docs.google.com/spreadsheets/d/1JfbGypt0GTB7GsWNfdcwVB3hlSbqePDoDFGnf-xNzBA/edit?usp=sharing"
      },
      "contacts" : {
      "handler": "csv",
      "source": "test_corentin/metadata/osur/metadata_contacts_geoflow.csv"
    }
  },
  "software": [
    {
		"id": "geosur-geonetwork",
		"type": "output",
		"software_type": "geonetwork",
		"parameters": {
			"url": "{{GEONETWORK_DATACITE_URL}}",
			"user": "{{GEONETWORK_DATACITE_USER}}",
			"pwd": "{{GEONETWORK_DATACITE_PASSWORD}}",
			"version": "4.2.1",
			"logger": "DEBUG"
		}
	},
	    	{
		"id": "googledrive",
		"type": "input",
		"software_type": "googledrive",
		"parameters": {
    "email": "{{GMAIL_USER}}",
			"token": ""
		},
		"properties" : {}
	},
	{
        "id": "geosur-geoserver",
        "type": "output",
        "software_type": "geoserver",
        "parameters": {
			"url": "{{GEOSERVER_UNIV_URL}}",
			"user": "{{GEOSERVER_USER}}",
			"pwd": "{{GEOSERVER_PASSWORD}}",
			"logger": "DEBUG"
        },
      "properties" : {
			"workspace": "OSU_R",
			"store": "eobs"
      }
    }
  ],
 "actions": [
    {
      "id": "geometa-create-iso-19115",
      "run": true,
            "options": {
            "logo": true,
            "doi": false,
            "doi_thumbnail": false,
            "addfeatures": false
        }
    },
        {
      "id": "geonapi-publish-iso-19139",
      "run" : true,
      "options": {
        "create_doi_on_datacite": true
      }
	   },
	 {
	  "id": "geosapi-publish-ogc-services",
	  "run": true,
	  "options" : {
	    "createWorkspace": true,
	    "createStore": true,
	   "overwrite_upload": false
	  }
	}
  ]
}